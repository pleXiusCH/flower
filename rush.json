{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.39.1",
  "pnpmVersion": "5.15.2",
  "pnpmOptions": {
  "pnpmStore": "local",
  "resolutionStrategy": "fewer-dependencies",
  "preventManualShrinkwrapChanges": true,
  "useWorkspaces": true
  },
  "repository": {
    "url": "https://github.com/pleXiusCH/flower",
    "defaultBranch": "master",
    "defaultRemote": "origin"
  },
  "projects": [
    {
      "packageName": "@plexius/flower-interfaces",
      "projectFolder": "packages/flower-interfaces"
    },
    {
      "packageName": "@plexius/flower-nodes",
      "projectFolder": "packages/flower-nodes"
    },
    {
      "packageName": "@plexius/flower-core",
      "projectFolder": "packages/flower-core"
    },
    {
      "packageName": "@plexius/flower-react-editor",
      "projectFolder": "packages/flower-react-editor"
    }
  ]
}
